<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="230dp"
    android:layout_margin="6dp">

    <View
        android:id="@+id/view_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/bg_timer_retcangle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_timer_header_1"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="@drawable/bg_timer_header"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintStart_toStartOf="@id/view_1"
        app:layout_constraintTop_toTopOf="@id/view_1"
        app:layout_constraintVertical_bias="0.0" />


    <TextView
        android:id="@+id/timer_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <TextView
        android:id="@+id/txt_timer_header_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Morning Timer"
        android:textColor="@color/white"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@id/view_timer_header_1"
        app:layout_constraintEnd_toEndOf="@id/view_timer_header_1"
        app:layout_constraintStart_toStartOf="@id/view_timer_header_1"
        app:layout_constraintTop_toTopOf="@id/view_timer_header_1" />

    <TextView
        android:id="@+id/txt_output_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="5dp"
        android:text="Output"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@id/txt_output_value_1"
        app:layout_constraintStart_toStartOf="@id/view_timer_header_1"
        app:layout_constraintTop_toBottomOf="@id/view_timer_header_1" />

    <TextView
        android:id="@+id/txt_output_value_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Output 6"
        android:textColor="@color/primary"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@id/txt_mode_name_1"
        app:layout_constraintStart_toStartOf="@id/txt_output_name_1"
        app:layout_constraintTop_toBottomOf="@id/txt_output_name_1" />

    <View
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:background="@drawable/timer"
        app:layout_constraintBottom_toTopOf="@+id/view_vertical_timer_1_2"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toEndOf="@+id/txt_mode_value_1"
        app:layout_constraintTop_toBottomOf="@id/view_timer_header_1"
        app:layout_constraintVertical_bias="0.4" />

    <TextView
        android:id="@+id/txt_mode_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Mode"
        android:textColor="@color/black"
        app:layout_constraintBottom_toTopOf="@id/txt_mode_value_1"
        app:layout_constraintStart_toStartOf="@id/txt_output_value_1"
        app:layout_constraintTop_toBottomOf="@id/txt_output_value_1" />


    <TextView
        android:id="@+id/txt_mode_value_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="off Before"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/txt_mode_name_1"
        app:layout_constraintTop_toBottomOf="@id/txt_mode_name_1" />

    <View
        android:id="@+id/view_hor_1_1"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="3dp"
        android:background="@color/black"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintStart_toStartOf="@id/view_1"
        app:layout_constraintTop_toBottomOf="@id/txt_mode_value_1" />

    <TextView
        android:id="@+id/timer_start_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="StatTime"
        android:textColor="@color/black"
        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/view_vertical_timer_1_1"
        app:layout_constraintStart_toStartOf="@id/view_1"
        app:layout_constraintTop_toBottomOf="@id/view_hor_1_1" />

    <TextView
        android:id="@+id/timer_start_value_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="01:05:56"
        android:textColor="@color/primary"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/timer_start_name_1"
        app:layout_constraintStart_toStartOf="@id/timer_start_name_1"
        app:layout_constraintTop_toBottomOf="@id/timer_start_name_1" />

    <View
        android:id="@+id/view_vertical_timer_1_1"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:visibility="gone"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintHorizontal_bias="0.33"
        app:layout_constraintStart_toStartOf="@id/view_1"
        app:layout_constraintTop_toBottomOf="@id/view_hor_1_1" />


    <TextView
        android:id="@+id/timer_duration_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Duration"
        android:textColor="@color/black"
        android:visibility="gone"
        app:layout_constraintEnd_toStartOf="@+id/view_vertical_timer_1_2"
        app:layout_constraintStart_toEndOf="@id/view_vertical_timer_1_1"
        app:layout_constraintTop_toBottomOf="@id/view_hor_1_1" />

    <TextView
        android:id="@+id/timer_duration_value_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="01:05:56"
        android:textColor="@color/primary"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/timer_duration_name_1"
        app:layout_constraintStart_toStartOf="@id/timer_duration_name_1"
        app:layout_constraintTop_toBottomOf="@id/timer_duration_name_1" />


    <View
        android:id="@+id/view_vertical_timer_1_2"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:background="@color/black"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintHorizontal_bias="0.66"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/view_1"
        app:layout_constraintTop_toBottomOf="@id/view_hor_1_1" />


    <TextView
        android:id="@+id/timer_status_name_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Status"
        android:textColor="@color/black"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="@id/view_1"
        app:layout_constraintStart_toEndOf="@id/view_vertical_timer_1_2"
        app:layout_constraintTop_toBottomOf="@id/view_hor_1_1" />

    <TextView
        android:id="@+id/timer_status_value_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="Running"
        android:textColor="@color/primary"
        android:textStyle="bold"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/view_1"
        app:layout_constraintEnd_toEndOf="@id/timer_status_name_1"
        app:layout_constraintStart_toStartOf="@id/timer_status_name_1"
        app:layout_constraintTop_toBottomOf="@id/timer_status_name_1" />


</androidx.constraintlayout.widget.ConstraintLayout>